{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/auth/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/logs/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/users/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/categories/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/items/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/reservations/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/menu/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/timeslots/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/billings/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/inventory/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/usage/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/offer-sections/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}